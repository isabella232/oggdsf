#
# Export file for libfishsound
#
# Only the symbols listed in the global section will be callable from
# applications linking to libfishsound.
#

@PACKAGE@.so
{
        global:
		fish_sound_identify;
		fish_sound_new;
		fish_sound_set_decoded_callback;
		fish_sound_set_encoded_callback;
		fish_sound_decode;
		fish_sound_encode;
		fish_sound_reset;
		fish_sound_flush;
		fish_sound_delete;
		fish_sound_command;
		fish_sound_get_interleave;
		fish_sound_set_interleave;
        local:
                *;
};
